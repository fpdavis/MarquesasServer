  <data name="HTML_Manual" xml:space="preserve">
    <value>&lt;!DOCTYPE html&gt;
&lt;html&gt;
&lt;head&gt;
&lt;meta http-equiv="content-type"
content="text/html; charset=windows-1252"&gt;
&lt;title&gt;Game Manual&lt;/title&gt;

&lt;script src="https://code.jquery.com/jquery-3.2.1.min.js" integrity="sha256-hwg4gsxgFZhOsEEamdOYGBf13FyQuiTwlAQgxVSNgt4=" crossorigin="anonymous"&gt;&lt;/script&gt;

&lt;/head&gt;
&lt;body&gt;
&lt;div id="pdf" style="width:100%;height:100%"&gt;&lt;/div&gt;

&lt;script type="text/javascript"&gt;

var oTimer = null;
var sID = null

function refresh() {
document.getElementById("pdf").innerHTML = "&lt;embed src=\"&lt;!-- BinaryPath --&gt;\" type=\"application/pdf\" style=\"position:absolute\" width=\"100%\" height=\"100%\" /&gt;";
}

function autoRefresh() {
oTimer = setTimeout("autoRefresh()", &lt;!-- SecondsBetweenRefresh --&gt;);
CheckForUpdate();
}

function CheckForUpdate() {
var sURL = "/selectedgames/id";
var xmlRequest = $.ajax({
    url: sURL,
    type: 'GET',
    dataType: 'json',
    contentType: 'application/json',
	 error: function(XMLHttpRequest, textStatus, errorThrown) { 
        location = "/manual"; 
    },
    success: function(data) { 
	   if (data)
          {	   
	       if (sID != data)
	          {
	           sID = data;
	           refresh();
		      }
		  }
      else
	      {
           location = "/manual";
          }
	}
    });
	
}

autoRefresh();
&lt;/script&gt;
&lt;/body&gt;
&lt;/html&gt;</value>
